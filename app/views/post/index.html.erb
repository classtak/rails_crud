<h2>게시판입니다.</h2>
<p><a href="/post/new">글쓰기</a></p>
<hr />
<!-- bootstrap 코드 링크
https://getbootstrap.com/docs/4.0/content/tables/#examples -->
<table class="table">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">작성자</th>
      <th scope="col">제목</th>
      <th scope="col">내용</th>
      <th scope="col">작성시간</th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
    <tr>
      <th scope="row"><%= post.id %></th>
      <td><%= post.username %></td>
      <td><%= post.title %></td>
      <td><%= post.content %></td>
      <td><%= post.created_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>
